{
  "name": "last-man-telegram-webapp",
  "version": "1.0.0",
  "description": "Telegram WebApp для составления программ тренировок на основе СРЦ (саморасчитывающихся циклов)",
  "main": "src/webapp.js",
  "scripts": {
    "start": "node src/webapp.js",
    "dev": "nodemon src/webapp.js",
    "bot": "node src/bot.js",
    "bot:dev": "nodemon src/bot.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "security:check": "npm audit --audit-level moderate",
    "test:all": "npm run lint",
    "vercel-build": "echo 'Build completed'"
  },
  "keywords": [
    "telegram",
    "bot",
    "fitness",
    "training",
    "workout"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.1"
  },
  "overrides": {
    "form-data": ">=2.5.4",
    "tough-cookie": ">=4.1.3",
    "request": ">=2.88.2"
  }
}
